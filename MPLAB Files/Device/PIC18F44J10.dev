	# Access mask for this bit field is RP (Read/Programmable bit)
    field (key=CP_0 mask=0x4 desc="Code Protect Enable")
        setting (req=0x4 value=0x4 desc="Disabled")
            checksum (type=0x0 protregion=0x00-0x00)
