        setting (req=0x4 value=0x0 desc="Off")
    field (key=PUT mask=0x8 desc="Power Up Timer")
        setting (req=0x8 value=0x8 desc="Off")
        setting (req=0x8 value=0x0 desc="On")
    field (key=BODEN mask=0x40 desc="Brown Out Detect")
