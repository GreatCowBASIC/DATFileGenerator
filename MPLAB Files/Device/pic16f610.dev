        portpins (muxaddr=0x1A muxmask=0x03)
            setting (muxval=0x0 port=RA1 dir=in)
            setting (muxval=0x1 port=RC1 dir=in)
            setting (muxval=0x2 port=RC2 dir=in)
            setting (muxval=0x3 port=RC3 dir=in)
