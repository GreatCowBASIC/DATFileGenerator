          <claimResource type="register" registerField="ACQT1" value="0"></claimResource>
          <claimResource type="register" registerField="ACQT0" value="0"></claimResource>
        </userAction>
        <userAction name="6TAD">
          <claimResource type="register" registerField="ACQT2" value="0"></claimResource>
          <claimResource type="register" registerField="ACQT1" value="1"></claimResource>
          <claimResource type="register" registerField="ACQT0" value="1"></claimResource>
        </userAction>
        <userAction name="4TAD">
          <claimResource type="register" registerField="ACQT2" value="0"></claimResource>
          <claimResource type="register" registerField="ACQT1" value="1"></claimResource>
          <claimResource type="register" registerField="ACQT0" value="0"></claimResource>
        </userAction>
        <userAction name="2TAD">
          <claimResource type="register" registerField="ACQT2" value="0"></claimResource>
          <claimResource type="register" registerField="ACQT1" value="0"></claimResource>
          <claimResource type="register" registerField="ACQT0" value="1"></claimResource>
        </userAction>
        <userAction name="0TAD">
          <claimResource type="register" registerField="ACQT2" value="0"></claimResource>
          <claimResource type="register" registerField="ACQT1" value="0"></claimResource>
          <claimResource type="register" registerField="ACQT0" value="0"></claimResource>
        </userAction>
      </userControl>

      <userControl name="acqPeriod">
        <dialogConfig type="TB" caption="Sample Period"></dialogConfig>
        <userAction>
	<!-- TAD=TCY*(0.5*(ADCS[5:0]+1)) -->
	<!-- min=TCY/2, max=32*TCY -->
        </userAction>
      </userControl>

    </userControlSubGroup>

  </userControlGroup>

